[avalanche-testnet]
avalanche-axelar-testnet ansible_host=10.0.10.30 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/axelar-testnet.pem
[avalanche-testnet:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ProxyCommand="ssh -W %h:%p -q bastion-axelar-testnet"'
chain_id='fuji'
env='axelar-testnet'

